  <Example>
    
    
    <i>/* Flash 5 Source code / Multiline comments */</i>
    <i>var xSpeed:Number = 3;</i>
    <i>var stageWidth:Number = 550;</i>
    <i>var stageHeight:Number = 580;</i>
    <i/>
    <i>listA_ls.dataProvider = [{data:0, label:"January"}, </i>
    <i> {data:1, label:"February"}, </i>
    <i> {data:2, label:"March"}, </i>
    <i> {data:3, label:"April"}, </i>
    <i> {data:4, label:"May"}, </i>
    <i> {data:5, label:"June"}, </i>
    <i> {data:6, label:"July"}, </i>
    <i> {data:7, label:"August"}, </i>
    <i> {data:8, label:"September"}, </i>
    <i> {data:9, label:"October"}, </i>
    <i> {data:10, label:"November"}, </i>
    <i> {data:11, label:"December"}];</i>
    <i/>
    <i/>
    <i>/* define the function which will be used to sort the items in the lists. </i>
    <i>   This function is used by both sort buttons on the Stage. */</i>
    <i>function sortByMonth(a:Object, b:Object):Boolean </i>
    <i> {</i>
    <i><![CDATA[   return parseInt(a.data)>parseInt(b.data);]]></i>
    <i> }</i>
    <i/>
    <i>mouseListener.onMouseMove = function() </i>
    <i> {</i>
    <i>   crosshair_mc._x = _xmouse;</i>
    <i>   crosshair_mc._y = _ymouse;</i>
    <i> };</i>
  </Example>
