  <Rules>
    
    <Rule>
      <Name>String</Name>
      <Starter>"</Starter>
      <Ender>"</Ender>
      <Style>3</Style>
      <CloseOnDelimeter>No</CloseOnDelimeter>
      <CloseOnEndLine>No</CloseOnEndLine>
    </Rule>
    <Rule>
      <Name>Comment</Name>
      <Starter>//</Starter>
      <Ender/>
      <Style>5</Style>
      <CloseOnDelimeter>No</CloseOnDelimeter>
      <CloseOnEndLine>Yes</CloseOnEndLine>
    </Rule>
