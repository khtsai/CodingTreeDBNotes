  <Rules>
    
    
    
    
    
    
    <Rule>
      <Name>Tags</Name>
      <Starter><![CDATA[<]]></Starter>
      <Ender><![CDATA[>]]></Ender>
      <Style>6</Style>
      <CloseOnDelimeter>No</CloseOnDelimeter>
      <CloseOnEndLine>No</CloseOnEndLine>
      <Force>No</Force>
    </Rule>
    <Rule>
      <Name>Style</Name>
      <Starter><![CDATA[<style]]></Starter>
      <Ender><![CDATA[style>]]></Ender>
      <Style>10</Style>
      <CloseOnDelimeter>No</CloseOnDelimeter>
      <CloseOnEndLine>No</CloseOnEndLine>
      <Force>No</Force>
    </Rule>
    <Rule>
      <Name>Style Zone</Name>
      <Starter>{</Starter>
      <Ender>}</Ender>
      <Style>2</Style>
      <CloseOnDelimeter>No</CloseOnDelimeter>
      <CloseOnEndLine>No</CloseOnEndLine>
      <Force>No</Force>
    </Rule>
    <Rule>
      <Name>String</Name>
      <Starter>"</Starter>
      <Ender>"</Ender>
      <Style>3</Style>
      <CloseOnDelimeter>No</CloseOnDelimeter>
      <CloseOnEndLine>No</CloseOnEndLine>
      <Force>Yes</Force>
    </Rule>
    <Rule>
      <Name>Color</Name>
      <Starter>#</Starter>
      <Ender/>
      <Style>1</Style>
      <CloseOnDelimeter>Yes</CloseOnDelimeter>
      <CloseOnEndLine>No</CloseOnEndLine>
      <Force>No</Force>
    </Rule>
    <Rule>
      <Name>Special Symbol</Name>
      <Starter>&</Starter>
      <Ender>;</Ender>
      <Style>1</Style>
      <CloseOnDelimeter>No</CloseOnDelimeter>
      <CloseOnEndLine>No</CloseOnEndLine>
      <Force>No</Force>
    </Rule>
    <Rule>
      <Name>Comments</Name>
      <Starter><![CDATA[<!--]]></Starter>
      <Ender><![CDATA[-->]]></Ender>
      <Style>5</Style>
      <CloseOnDelimeter>No</CloseOnDelimeter>
      <CloseOnEndLine>No</CloseOnEndLine>
      <Force>Yes</Force>
    </Rule>
  </Rules>
