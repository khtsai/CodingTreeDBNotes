  <Rules>
    
    
    
    
    
    <Rule>
      <Name>Key path</Name>
      <Starter>[</Starter>
      <Ender>]</Ender>
      <Style>3</Style>
      <CloseOnDelimeter>No</CloseOnDelimeter>
      <CloseOnEndLine>No</CloseOnEndLine>
      <Force>No</Force>
    </Rule>
    <Rule>
      <Name>CLSID</Name>
      <Starter>{</Starter>
      <Ender>}</Ender>
      <Style>1</Style>
      <CloseOnDelimeter>No</CloseOnDelimeter>
      <CloseOnEndLine>No</CloseOnEndLine>
      <Force>No</Force>
    </Rule>
    <Rule>
      <Name>Hex</Name>
      <Starter>=hex</Starter>
      <Ender/>
      <Style>5</Style>
      <CloseOnDelimeter>Yes</CloseOnDelimeter>
      <CloseOnEndLine>Yes</CloseOnEndLine>
      <Force>No</Force>
    </Rule>
    <Rule>
      <Name>DWord</Name>
      <Starter>dword:</Starter>
      <Ender/>
      <Style>5</Style>
      <CloseOnDelimeter>Yes</CloseOnDelimeter>
      <CloseOnEndLine>Yes</CloseOnEndLine>
      <Force>No</Force>
    </Rule>
    <Rule>
      <Name>String</Name>
      <Starter>"</Starter>
      <Ender>"</Ender>
      <Style>7</Style>
      <CloseOnDelimeter>No</CloseOnDelimeter>
      <CloseOnEndLine>No</CloseOnEndLine>
      <Force>Yes</Force>
    </Rule>
  </Rules>
